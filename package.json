{
  "name": "schedule-bot",
  "version": "1.0.0",
  "description": "Discord Bot for Valorant Team Schedule Management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "prisma generate && tsc",
    "start": "node dist/index.js",
    "dev": "prisma generate && tsc && node dist/index.js",
    "deploy": "prisma migrate deploy && npm run build",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "discord",
    "discord-bot",
    "esports",
    "valorant",
    "schedule",
    "team-management",
    "availability",
    "dashboard"
  ],
  "author": "jonax1337",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonax1337/Schedule-Bot.git"
  },
  "bugs": {
    "url": "https://github.com/jonax1337/Schedule-Bot/issues"
  },
  "homepage": "https://github.com/jonax1337/Schedule-Bot#readme",
  "dependencies": {
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "joi": "^18.0.2",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.13.1",
    "prisma": "^7.3.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.10",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.11.10",
    "@vitest/coverage-v8": "^4.0.18",
    "vitest": "^4.0.18"
  }
